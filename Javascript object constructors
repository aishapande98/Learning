<h2>JavaScript Object Constructors</h2>

<p id="demo"></p>

<script>
// Constructor function for Employee object
function Employee(first, last, Desig, Sal) {
  this.firstName = first;
  this.lastName = last;
  this.Designation = Desig;
  this.Salary = Sal;
}

// Create a employee object
var EmpSalary = new Employee("John", "Doe", "Developer", 25000);

document.getElementById("demo").innerHTML =
"My salary is " + EmpSalary.Salary + "."; 
</script>

</body>
</html>
