<!DOCTYPE html>
<html>
<body>


<p id="demo"></p>

<script>
// Create an object:
var employee = {
  firstName: "John",
  lastName : "Doe",
  Designation : "Software developer",
  set Desig(Desig) {
    this.Designation=Desig;
  }
};
employee.Desig="Software developer";
// Display data from the object using a getter:
document.getElementById("demo").innerHTML = employee.Designation;
</script>

